{
  "name": "simp-simp_elasticsearch",
  "version": "4.1.0",
  "author": "SIMP Team",
  "summary": "SIMP module for integrating elastic/puppet-elasticsearch",
  "license": "Apache-2.0",
  "source": "https://github.com/simp/pupmod-simp-simp_elasticsearch",
  "project_page": "https://github.com/simp/pupmod-simp-simp_elasticsearch",
  "issues_url": "https://simp-project.atlassian.net",
  "tags": [
    "simp",
    "logstash",
    "elasticsearch",
    "es"
  ],
  "dependencies": [
    {
      "name": "elasticsearch/elasticsearch",
      "version_requirement": ">= 0.11.0 < 1.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.13.0 < 5.0.0"
    },
    {
      "name": "simp/simp_apache",
      "version_requirement": ">= 6.0.0 < 7.0.0"
    },
    {
      "name": "simp/simplib",
      "version_requirement": ">= 3.1.0 < 4.0.0"
    },
    {
      "name": "simp/iptables",
      "version_requirement": ">= 6.0.0 < 7.0.0"
    },
    {
      "name": "puppetlabs/java",
      "version_requirement": ">= 1.6.0 < 2.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": "4.x"
    },
    {
      "name": "pe",
      "version_requirement": ">= 2016.2.0"
    }
  ],
  "data_provider": "hiera"
}
